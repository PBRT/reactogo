.spinner-path
  stroke-dasharray: 612
  stroke-dashoffset: 612
  animation: dash 2.5s linear alternate infinite


@keyframes dash
  0% {
    stroke-dashoffset: 612;
    fill: #ffffff;
  }

  5% {
    stroke-dashoffset: 612;
    fill: #ffffff;
  }

  90% {
    stroke-dashoffset: 0;
    fill: #01DEAC;
  }

  100% {
    stroke-dashoffset: 0;
    fill: #01DEAC;
  }
